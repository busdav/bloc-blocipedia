<div class="container">
  <div class="span5">
    <%= form_for @wiki do |f| %>
      <div class="control-group">
        <%= f.label :title %>
        <div class="controls">
          <%= f.text_field :title, :autofocus => true, placeholder: 'Enter Title' %>
        </div>
      </div>

      <div class="control-group">
        <%= f.label :body %>
        <div class="controls">
          <%= f.text_area :body %>
        </div>
      </div>

      <div class="control-group">
        <%= f.submit "Create Wiki!", class: 'btn' %></div>
    <% end %>
  </div>
  <div class="span5">
    <h1>EDIT VIEW HERE</h1>
  </div>
</div>